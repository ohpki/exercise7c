<%= form_with model:group,local:true do |f| %>
  <div class="form-group">
    <%= f.label :name %>
     <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :introduction %>
     <%= f.text_area :introduction %>
  </div>
  <div class="form-group">
    <%= f.label :name %>
      <%= f.file_field :group_image %>
  </div>
  <div class="form-group">
    <%= f.label :name %>
      <%= f.submit class: 'btn btn-secondary' %>
  </div>
  <% end %>
